{
  "name": "openjs-foundation",
  "version": "1.0.0",
  "description": "Secure Dashboard for the OpenJS Foundation",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --runInBand --verbose --coverage",
    "infra:start": "docker-compose up -d",
    "infra:stop": "docker-compose down",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:generate-schema": "docker-compose run schema-dump",
    "db:seed": "knex seed:run"
  },
  "keywords": [],
  "author": "Ulises Gasc√≥n",
  "license": "MIT",
  "standard": {
    "env": [
      "jest"
    ]
  },
  "dependencies": {
    "@ulisesgascon/normalize-boolean": "2.0.0",
    "@ulisesgascon/simplify-object": "2.0.0",
    "@ulisesgascon/string-to-array": "2.0.0",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "commander": "12.1.0",
    "debug": "4.3.7",
    "inquirer": "12.1.0",
    "knex": "3.1.0",
    "octokit": "3.2.1",
    "pg": "8.13.1",
    "validator": "13.12.0"
  },
  "devDependencies": {
    "jest": "29.7.0",
    "standard": "17.1.2"
  }
}
